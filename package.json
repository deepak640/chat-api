{
  "name": "cavalo_backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "commonjs",
  "engines": {
    "node": ">=20 <21"
  },
  "scripts": {
    "start": "cross-env NODE_PATH=./dist node ./dist/server.js",
    "prettier:write": "prettier --write .",
    "build": "tsc",
    "vercel-build": "npm run build",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "cross-env NODE_PATH=./dist node --enable-source-maps --watch dist/server.js",
    "dev:alt": "concurrently --names \"tsc,node\" -c \"blue,green\" \"npm run dev:tsc\" \"npm run dev:node\"",
    "dev": "nodemon -e ts --watch src --exec \"npm run build && npm start\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@azure/storage-blob": "^12.27.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.16",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^8.13.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
